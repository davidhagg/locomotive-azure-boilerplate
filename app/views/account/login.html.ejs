<% layout('../layout') -%>

<h1>Login</h1>

<form action="/login" method="POST">

    <div>
        <input type="text" placeholder="Email" name="email">
    </div>                    
    <div>
        <input type="password" placeholder="Password" name="password">
    </div>                    

    <div>
        <button type="submit">Login</button>
    </div>

</form>